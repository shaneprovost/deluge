# AWS (required for DynamoDB)
# Region must match where the CDK stack was deployed (e.g. us-east-1)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_key
AWS_SECRET_ACCESS_KEY=your_secret

# Table prefix must match stack: deluge-{env} â†’ deluge-dev, deluge-staging, deluge-prod
DYNAMODB_TABLE_PREFIX=deluge-dev

# Mapbox (for map view)
NEXT_PUBLIC_MAPBOX_TOKEN=your_mapbox_token
